#include <stdio.h>

int main() 
{
int n, first = 0, second = 1, next, i = 0;
printf("Enter the number of terms: ");
scanf("%d", &n);
if (n <= 0) 
{
printf("Please enter a positive integer.\n");
} 
else 
{
while (i < n) 
{
if (i == 0) 
{
next = first;
} 
else if (i == 1) 
{
next = second;
} 
else 
{
next = first + second;
first = second;
second = next;
}
printf("%d ", next);
i++;
}
printf("\n");
}
return 0;
}
