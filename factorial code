#include<stdio.h>

int main()
{
int num,i;
unsigned long long fact=1;
printf("Enter the number you want to find factorial for: ");
scanf("%d",&num);
if(num<=0)
{
printf("Factorial is not defined for negative numbers.\n");
}

else
{
for(int i=1;i<=num;i++)
{
fact = fact*i;
}
}
printf("The factorial of %d is: %llu \n",num,fact);
return 0;
}
